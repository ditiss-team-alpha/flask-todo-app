
#Policy change for static hosting in s3 
{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Statement1",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "S3:GetObject",
            "Resource": "arn:aws:s3:::ram-my-second-backet/*"
        }
    ]
}

#in place of resource put you bucket name


